    "del-cli": "1.1.0",
    "istanbul": "0.4.5",
    "karma": "3.0.0",
    "karma-browserify": "5.3.0",
    "karma-chai": "0.1.0",
    "karma-firefox-launcher": "1.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "nyc": "12.0.2",
    "ts-loader": "4.5.0",
